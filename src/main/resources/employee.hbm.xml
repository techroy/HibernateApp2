<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.hibernateapp2.model.EmployeeBean" table="Employee">

		<id name="eid" column="id" >
				<!-- <generator class="org.hibernate.id.Assigned"></generator> -->
				 <generator class="org.hibernate.id.IncrementGenerator"></generator> 
				
		</id>

		<property name="ename" column="name" />
		<property name="esal" column="sal" />

	</class>

</hibernate-mapping>